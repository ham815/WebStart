	//장바구니 추가
	public void cartInsert(CartVO vo);
	
	//장바구니 목록
	public List<CartVO> cartList(String cart_num);
	
	//장바구니 개별 삭제
	public String cartDelete();
	
	//장바구니 비우기
	public String cartDeleteAll();
	
	//장바구니 금액 합계
	
	//장바구니 상품 개수 
	
	//장바구니 수정 
	public String cartUpdate();
	
	
	

}
